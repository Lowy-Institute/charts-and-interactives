.container
  footer#footer
    .scroll-prompt.txt-sm.caption(style="text-align:center; left:200px; right: 200px;") Scroll down to start

    include social

    nav.pager
      .pager-page
        span.pager-page-start
        span.pager-page-middle
        span.pager-page-end

      ul.pager-list
        for item, i in new Array(15)
          li.pager-item
            a.pager-link(href=`#scroll-item-${i + 1}` tabindex="0")

