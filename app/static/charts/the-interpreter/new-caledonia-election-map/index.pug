    .fixed-bl
    
      ul.legend.txt-nowrap.flex.flex-cv.list-bare
        for item, i in ["#003B36", "#5B7E67","#9C9E93","#ff6f30","#d74000"]
          li.legend-item
            span.legend-item-box(style=`background:${item}`)
            span.legend-item-label!= `${i*20}&ndash;${20+i*20}%`
            
      .input.flex.flex-cv
        .radio-title.txt-tu.txt-xs.txt-bold
          | Dataset
        .row
          input(type="radio" id="pop" name="select" value="pop" checked)
          label(for="pop") Kanak population
        .row
          input(type="radio" id="vote" name="select" value="vote")
          label(for="vote") Yes vote to the referendum
      

    
  .tooltip
    .tt-title.txt-bold
    .tt-item-box
    span.tt-item
    span % Kanak population<br>
    .tt-item-box
    span.tt-item
    span % vote yes for independence
      
  
  script(src="script.js")

    

block append css

  style
    :sass

      .wrapper
        max-width: 800px
        height: 660px
        overflow: hidden
        position: relative
      
      .chart-title
        line-height: 1.1
        font-weight: 700
        color: $color-text
        font-size: 1.8rem
        max-width: 450px
        
      .chart-subtitle
        margin-bottom: 12px
        font-weight: 700
        font-size: 1.2rem
        max-width: 450px
      
      .inset
        position: absolute
        top: 0
        right: 0
        width: 130px
      
      .fixed-br
        position: absolute
        bottom: 0
        right: 0  
        
      .fixed-bl
        position: absolute
        bottom: 0
        left: 0  
        
      .legend
        font-size: 15px
        max-width: 320px
        margin-bottom: 24px

      .legend-item
        margin: 4px 8px 0
        padding-left: 28px
        position: relative

      .legend-item-box
        position: absolute
        left: 0
        top: 5px
        height: 10px
        width: 20px
      
      .tooltip
        position: absolute
        background: #fffe
        padding: .75em
        opacity: 0
        border-radius: .2em
        box-shadow: 0 0 5px 0 #0004
        transition-duration: 0.5s
        pointer-events: none
        font-size: 14px
        box-sizing: border-box

      .tt-item-box
        display: inline-block
        height: 10px
        width: 20px
        margin-right: 10px
      
      .input
        padding: 0.75em 1em
        box-shadow: 0 0 5px 1px #90909088
        margin: 0 0 5px 5px
      
      .radio-title
        margin-bottom: 6px
      
      .txt-xs
        font-size: 0.8rem
      
      .fade
        opacity: 0.2

      label
        font-size: 18px
      
      text
        font-size: 12px
        font-family: $font-default-stack

      line
        stroke: black
        stroke-width: 0.5px

      #map path
        fill: grey
        stroke: white
        stroke-width: 0.5px
        transition-duration: 0.3s

      circle
        display: none
        
      @media (max-width: 900px)
        .wrapper
          padding-right: 40px
        
      @media (max-width: 570px)
      
        .chart-title
          font-size: 1.5rem
      
      @media (max-width: 500px)
        
        .wrapper
          padding-right: 0
        
        .chart-title
          text-align: center

        .inset
          margin: auto
          position: relative
          display: block
          width: 100px
          
        .input
          width: 250px
          margin-left: auto
          margin-right: auto
          
        .fixed-bl
          width: 100%
          
          > *
            text-align: center
            
        .legend
          max-width: 140px
            
        label
          font-size: 17px
          
        .row:first-child
          margin-bottom: 10px
